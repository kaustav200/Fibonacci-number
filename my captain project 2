def print_positive_numbers(numbers):
    for num in numbers:
        if num > 0:
            print(num)

range1 = [12, -7, 5, 64, -14]
range2 = [12, 14, -95, 3]

print("Positive numbers in range 1:")
print_positive_numbers(range1)

print("Positive numbers in range 2:")
print_positive_numbers(range2)
